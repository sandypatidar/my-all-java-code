package in.india;
class MyThread extends Thread {
	public void run() {
		for (int i = 0; i < 10; i++) {
			System.out.println("hello is me");
		}
	}
}
public class ThreadDemo {

	public static void main(String[] args) {
		MyThread mt = new MyThread();
		Thread t = new Thread(mt);
		t.start();
		for (int i = 0; i < 10; i++) {
		System.out.println("ha sun liya");
		}
	}

}
